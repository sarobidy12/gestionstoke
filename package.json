{
  "name": "react-nodejs-example",
  "version": "1.0.0",
  "description": "example project react with nodejs",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "build": "cd front-end && npm run build",
    "install-front-end": "cd front-end && npm install",
    "dev": "nodemon ./server.js localhost 3080",
    "front-end": "cd front-end && npm start",
    "heruko-build":"npm run install-front-end && npm run build",
    "mac": "concurrently   -n, 'server,front-end'    -c, 'green,blue' \"nodemon server.js\" \"npm run front-end\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbachi/react-nodejs-example.git"
  },
  "author": "Bhargav Bachina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bbachi/react-nodejs-example/issues"
  },
  "homepage": "https://github.com/bbachi/react-nodejs-example#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.4",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.8",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.6",
    "webpack": "^4.43.0"
  }
}
